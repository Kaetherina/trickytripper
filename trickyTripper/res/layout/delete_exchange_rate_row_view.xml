<?xml version="1.0" encoding="utf-8"?>
<TableLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/manageExchangeRateRowView_upperLayout2"
    android:layout_width="fill_parent"
    android:layout_height="wrap_content"
    android:layout_gravity="center_vertical" >

    <TableRow
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:gravity="center" >

        <TableLayout
            android:id="@+id/manageExchangeRateRowView_upperLayoutNesrted"
            android:layout_width="0dip"
            android:layout_height="wrap_content"
            android:layout_gravity="fill_horizontal"
            android:layout_weight="1"
            android:paddingRight="10pt" >

            <TableRow>

                <TextView
                    android:id="@+id/manageExchangeRateRowView_output_from"
                    style="@style/LargeFont"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:gravity="left"
                    android:text="@string/defaultX" />

                <TextView
                    android:id="@+id/manageExchangeRateRowView_output_direction_indicator"
                    style="@style/LargeFont"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:gravity="center_horizontal" />

                <TextView
                    android:id="@+id/manageExchangeRateRowView_output_to"
                    style="@style/LargeFont"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:gravity="left"
                    android:text="@string/defaultX" />

                <TextView
                    android:id="@+id/manageExchangeRateRowView_output_main_rate"
                    android:layout_width="0dip"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"                    
                    android:gravity="right"
                    
                    android:text="@string/defaultX" />
            </TableRow>

            <TableRow>

                <TextView
                    android:id="@+id/manageExchangeRateRowView_output_from2"
                    style="@style/LargeFont"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:gravity="left"
                    android:text="@string/defaultX" />

                <TextView
                    android:id="@+id/manageExchangeRateRowView_output_direction_indicator2"
                    style="@style/LargeFont"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:gravity="center_horizontal" />

                <TextView
                    android:id="@+id/manageExchangeRateRowView_output_to2"
                    style="@style/LargeFont"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:gravity="left"
                    android:text="@string/defaultX" />

                <TextView
                    android:id="@+id/manageExchangeRateRowView_output_main_rate2"
                    android:layout_width="0dip"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:gravity="right"
                    android:text="@string/defaultX" />
            </TableRow>

            <TableRow>

                <TextView
                    android:id="@+id/manageExchangeRateRowView_output_comment"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_span="3"
                    android:gravity="left"
                    android:paddingLeft="10dp"
                    android:text="@string/defaultX" />
            </TableRow>
        </TableLayout>

        <CheckBox
            xmlns:android="http://schemas.android.com/apk/res/android"
            android:id="@+id/manageExchangeRateRowView_checkboxsssssssssssss"
            android:layout_width="fill_parent"
            android:layout_height="match_parent"
            android:checkMark="?android:attr/listChoiceIndicatorMultiple"
            android:gravity="center" />

    </TableRow>

</TableLayout>